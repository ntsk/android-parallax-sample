<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.sakebook.android.sample.parallaxsample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".acitvities.TopActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".acitvities.ImageActivity"
            android:theme="@style/TutorialTheme"/>
        <activity
            android:name=".acitvities.GradationActivity"
            android:theme="@style/TutorialTheme"/>
        <activity
            android:name=".acitvities.IndicatorActivity"
            android:theme="@style/TutorialTheme"/>
        <activity
            android:name=".acitvities.TransformIndicatorActivity"
            android:theme="@style/TutorialTheme"/>
        <activity
            android:name=".acitvities.ParallaxAnimationActivity"
            android:theme="@style/TutorialTheme"/>
        <activity
            android:name=".acitvities.TransitionAnimationActivity"
            android:theme="@style/TutorialTheme">
        </activity>
    </application>

</manifest>
